    <script>
      var bd = boydog(3090);
      
      var dogData = 'html';
      var dogLogic = {
        /*addAuth: function(data) {
          
          console.log("addAuth")
          
          return data;
        }*/
        "users": function(data) {
          
          return data;
        },
        "company": {
          "__u": function(data) {
            
            return data;
          },
          "__d": function(data) {
            
            return data;
          }
        },
        "features": {
          //Working middlewares
          "__middleU": function(data) {
            
            console.log("features middel up", data)
            
            data.set = data.set + "+";
            
            return data;
          },
          "__middleD": function(data) {
            
            console.log("features middel up")
            
            return data;
          },
          "body": {
            "__middleU": function(data) {
              
              console.log("features middel up body", data)
              data.set = data.set + "b";
              
              return data;
            },
            "up": { eyes: { '0': { color: {
              "__u": function(data) {
                
                console.log("executing last upload", data)
                
                data.set += ">";
                
                return data;
              }
            } } } }
          }
        }
      };
      
      bd.dogSet(dogData, dogLogic);
      
      $('#username').keyup(function() {
        console.log("bd", bd)
        
        bd.dogRebind();
      })
    </script>
  
