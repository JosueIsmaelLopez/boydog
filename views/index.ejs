    <script src="/socket.io.js"></script>
    <script src="/boydog-dog.js"></script>
    <script>
      var bd = boydog("http://192.168.1.68:3090/");
      
      var dogData = 'html';
      var dogLogic = {
        __upNext: function(data) {
          
          data.auth = { username: $('#username').val(), password: $('#password').val() }
          
          data.date = Date();
          
          return data;
        },
        __runNext: function(data) {
          
          console.log("__runNext .", data);
          data.auth = { username: $('#username').val(), password: $('#password').val() }
          
          return data;
        },
        users: {
          __down: {
            function(data) {
              
              console.log("users", data)
              
              return data;
            }
          }
        },
        company: {
          __up: function(data) {
            
            console.log("on __up", data)
            
            return data;
          },
          __down: function(data) {
            
            console.log("on __down")
            
            return data;
          }
        },
        addTask: {
          __run: function(data) {
            
            console.log("__runNext final", data)
            
            return data;
          }
        }
      };
      
      bd.dogSet(dogData, dogLogic);
      
      $('#username').keyup(function() {
        console.log("bd", bd)
        
        bd.dogRefresh();
      })
    </script>
  
